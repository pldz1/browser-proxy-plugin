{
  "manifest_version": 3,
  "name": "Browser_Proxy_Plugin",
  "version": "1.0.0",
  "permissions": ["proxy", "storage", "tabs"],
  "background": {
    "service_worker": "./modules/background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "incognito": "spanning"
}